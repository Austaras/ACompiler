// #[compiler_builtin::range_start=1]
struct NonZeroUSize {
    inner: uint
}

impl NonZeroUSize {
    pub fn new_unchecked(i: uint) -> Self {
        Self {
            inner: i
        }
    }

    pub fn add(self, count: uint) -> Self {
        Self {
            inner: self.inner + count
        }
    }

    pub fn unsafe_as_ref<T>(self) -> &T {
        self.inner as &T
    }
}

pub trait Num: Add + Sub + Mul + Div + Rem {}
