pub enum Option<T> {
    Some(T),
    None
}

impl<T> Option<T> {
    pub fn is_some(self) -> bool {
        match self {
            Some(_) => true,
            None => false
        }
    }

    pub fn is_none(self) -> bool {
        match self {
            Some(_) => false,
            None => true
        }
    }
}
