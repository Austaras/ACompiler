pub struct RawVec<T> {
    ptr: uint,
    len: uint,
    cap: uint
}

impl<T> RawVec<T> {
    pub fn malloc(size: uint) -> Self {
        Self {
            ptr: alloc::<T>(size),
            len: 0,
            cap
        }
    }

    pub fn realloc(&self, size: uint) {
        let new_ptr = realloc::<T>(self.ptr, size)

        self.ptr = new_ptr

        self.cap = cap

        if self.len > cap {
            self.len = cap
        }
    }
}
